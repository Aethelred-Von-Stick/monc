<data-definition name="instant_fields_3d" frequency="{sampling_frequency}">
	<field name="w" type="array" data_type="double" size="z,y,x" collective=true />
	<field name="u" type="array" data_type="double" size="z,y,x" collective=true />
	<field name="v" type="array" data_type="double" size="z,y,x" collective=true />
	<field name="q" type="array" data_type="double" size="x,y,z,qfields" collective=true />
</data-definition>

<group name="instantaneous_3d">
	<member name="w"/>
	<member name="u"/>
	<member name="v"/>
	<member name="q"/>
</group>

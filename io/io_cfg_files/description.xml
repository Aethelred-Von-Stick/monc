<io-configuration>
	<server-configuration>
		<thread_pool number=10/>
	</server-configuration>

#include "io/io_cfg_files/instantaneous_3d_winds.xml"
#include "io/io_cfg_files/instantaneous_3d_qs.xml"

	<data-writing>
		<file name="instantaneous_3d.nc" write_time_frequency="{diag_write_freq}">
		  <include group="instantaneous_winds" time_manipulation="instantaneous"  output_frequency="{mm}"/>
		  <include group="instantaneous_qs" time_manipulation="instantaneous"  output_frequency="{mm}"/>
		</file>
	</data-writing>

</io-configuration>
